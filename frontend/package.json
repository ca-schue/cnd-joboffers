    "@mui/material": "^5.14.10",
    "@reduxjs/toolkit": "^1.9.6",
    "axios": "^1.4.0",
    "blurhash": "^2.0.5",
    "bootstrap": "^5.3.2",
    "file-loader": "^6.2.0",
