    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-oidc-context": "^2.3.1",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.16.0",
    "redux": "^4.2.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "use-sound": "^4.0.1"
  },
